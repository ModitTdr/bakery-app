generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           String  @id @default(uuid())
  name         String
  email        String  @unique
  password     String
  refreshToken String? @unique

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
